class Caculator{
    public static int plus(int a,int b) {
       return a+b;
    }
    public static int minus(int a,int b) {
        return a-b;
    }
    public static double multiply(double a,double b) {
        return a*b;
    }
    public static double division(double j,double k){
        return j/k;
    }
}
