import java.util.Scanner;
public class Main {
        public static void main(String[] args){
          int a,b;
          double j,k;
          Scanner s = new Scanner(System.in);
          Caculator cac = new Caculator();
          System.out.println("Enter number of Caculate : ");
          System.out.print("Enter number a : ");
          a=s.nextInt();
          System.out.print("Enter number b : ");
          b=s.nextInt();
          System.out.print("Enter number j : ");
          j=s.nextDouble();
          System.out.print("Enter number k : ");
          k=s.nextDouble();
          System.out.println("Value of a+b is : "+ cac.plus(a,b));
          System.out.println("Value of a-b is : "+ cac.minus(a,b));
          System.out.println("Value of a*b is : "+ cac.multiply(a,b));
          System.out.println("Value of j/k is : "+ cac.division(j,k));
        
    }
  }
